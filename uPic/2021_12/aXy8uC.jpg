                    <input type="submit" name="reject" class="reject" value="Reject">
                    <input type="submit" name="assign" class="assign" disabled="disabled" value="Assign">
                    <a href="#" class="cancel-approval">Cancel</a>
                </div>
            </form>

        </div>
    </div>
</script>

